case $1 in
	netm)
		cd /home/lizheng/work/tr781/device/tp-link/net/netm
		;;
	wlan)
		cd /home/lizheng/work/tr781/device/tp-link/wlan/wlan_utilities
		;;
	inc)
		cd /home/lizheng/work/tr781/system/core/include/private
		;;
	out)
		cd /home/lizheng/work/tr781/out/debug/target/product/tr781/system/bin
		;;
    k)
        cd /usr/src/linux-headers-3.2.0-54/include/linux
        ;;


	*)
		echo "Usage:"
		echo "netm /home/lizheng/work/tr781/device/tp-link/net/netm"
		echo "wlan /home/lizheng/work/tr781/device/tp-link/wlan/wlan_utilities"
		echo "inc  /home/lizheng/work/tr781/system/core/include/private"
		echo "out  /home/lizheng/work/tr781/out/debug/target/product/tr781/system/bin"
		echo "k    /usr/src/linux-headers-3.2.0-54/include/linux"
		;;
esac
